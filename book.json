{
    "title": "blog",
    "author": "Giert",
    "description": "my notes",
    "extension": null,
    "generator": "site",
    "links": {
        "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        }
    },
    "pdf": {
        "fontSize": 18,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "advanced-emoji",
        "toggle-chapters",
        "theme-comscore",
        "splitter",
        "github",
        "-sharing",
        "-lunr",
        "-search",
        "search-plus",
        "anchor-navigation-ex-toc@0.0.9",
        "editlink",
        "copy-code-button",
        "prism",
        "-highlight",
        "advanced-emoji",
        "simple-page-toc",
        "local-video",
        "splitter"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/shln21/blogs"
        },
        "editlink": {
            "base": "https://github.com/shln21/blogs/blob/master",
            "label": "编辑本页"
        },
        "anchor-navigation-ex-toc": {
            "showLevel": false,
            "mode": "float",
            "float": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "prism": {
            "css": [
                "prism-themes/themes/prism-base16-ateliersulphurpool.light.css"
            ]
        },
        "simple-page-toc": {
            "maxDepth": 3,
            "skipFirstH1": true
        }
    }
}
